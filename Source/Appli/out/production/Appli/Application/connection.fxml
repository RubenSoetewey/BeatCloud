<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<DialogPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Application.ConnectionController">
   <content>
      <TabPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="63.0" prefWidth="600.0" style="-fx-background-color: #A9F5F2;" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Connect">
               <content>
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Label text="Login" />
                        <TextField />
                        <Label text="Password" />
                        <PasswordField />
                        <CheckBox mnemonicParsing="false" text="Remember me" />
                        <Hyperlink text="Forgot your password ?" />
                        <Button mnemonicParsing="false" text="Validate" />
                     </children></VBox>
               </content></Tab>
          <Tab text="Register">
               <content>
                  <VBox prefHeight="200.0" prefWidth="100.0" />
               </content></Tab>
        </tabs>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </TabPane>
   </content>
</DialogPane>
